<rules name="tic-tac-toe">
    <rule no="1" p1="0" p2="1" p3="2">horizontal</rule>
    <rule no="2" p1="3" p2="4" p3="5">horizontal</rule>
    <rule no="3" p1="6" p2="7" p3="8">horizontal</rule>
    <rule no="4" p1="0" p2="3" p3="6">vertical</rule>
    <rule no="5" p1="1" p2="4" p3="7">vertical</rule>
    <rule no="6" p1="2" p2="5" p3="8">vertical</rule>
    <rule no="7" p1="0" p2="4" p3="8">diagonal</rule>
    <rule no="8" p1="2" p2="4" p3="6">diagonal</rule>
</rules>